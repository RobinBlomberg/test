{
  "name": "@robinblomberg/test",
  "version": "0.5.8",
  "author": "Robin Blomberg",
  "type": "module",
  "main": "index.js",
  "types": "dist/index.d.ts",
  "dependencies": {
    "@robinblomberg/ansi": "^0.5.1",
    "@robinblomberg/assert": "^0.2.9",
    "@robinblomberg/timer": "^0.1.1"
  },
  "devDependencies": {
    "@robinblomberg/eslint-config-robinblomberg": "^0.2.20",
    "concurrently": "^5.3.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.15.0",
    "husky": "^4.3.4",
    "lint-staged": "^10.5.3"
  },
  "scripts": {
    "compile": "rimraf dist && tsc",
    "dev": "nodemon --watch . --exec \"concurrently \\\"node ./index.test.js\\\" tsc\"",
    "lint": "eslint \"**/*.{js,ts}\"",
    "lint:fix": "eslint \"**/*.{js,ts}\" --fix",
    "pre-commit": "concurrently --kill-others-on-fail \"npm run test\" eslint",
    "test": "node ./index.test.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run compile && npm run test && lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts}": "cross-env CI=true eslint --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RobinBlomberg/test.git"
  },
  "bugs": {
    "url": "https://github.com/RobinBlomberg/test/issues"
  },
  "homepage": "https://github.com/RobinBlomberg/test#readme"
}
